<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
</head>
<body ng-app="app" ng-controller="AppController">
    <div class="navbar">
        @Html.Partial("_Images")
    </div>

    <div class="container-fluid body-content">
        @RenderBody()
        <hr />
        <footer id="footer">
            <form id="image" class="form-inline">
                <label for="file">Upload File</label>
                <input name="file" type="file" id="file" get-file-exif placeholder="Upload File" class="form-control" /> <button upload-file class="btn btn-success" id="upload">Upload</button>
            </form>
        </footer>
    </div>
    <div ng-helper-busy></div>
    @Scripts.Render("~/bundles/jquery")
    <script src="/Scripts/scripts.js"></script>
    @Scripts.Render("~/Assets/plugins")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/Angular/")
    @Scripts.Render("~/AngularScript/")
    @Scripts.Render("~/AngularApp/")
    @RenderSection("scripts", required: false)
</body>
</html>